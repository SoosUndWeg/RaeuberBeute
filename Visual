    "#Fall 2\n",
    "def f2(r, t):\n",
    "        \n",
    "        alpha = 0.01\n",
    "        beta  = 0.4\n",
    "        delta = 0.01\n",
    "        gamma = 0.09\n",
    "        r1    = 0.8\n",
    "        r2    = 0.5\n",
    "        x, y = r[0], r[1]\n",
    "        \n",
    "        fxd = x * (r1  - alpha * x - gamma * y)\n",
    "        fyd = y * (-r2 + beta * x - delta * y)\n",
    "        return np.array([fxd, fyd], float)\n",
    "\n",
    "h=0.0001                               \n",
    "tpoints = np.arange(0, 30, h)         \n",
    "xpoints, ypoints  = [], []\n",
